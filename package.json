{
  "name": "beware-blocks",
  "version": "0.0.5",
  "description": "a falling block puzzle game using cycle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeremylarter/beware-blocks"
  },
  "contributors": [
    {
      "name": "Jeremy Larter",
      "email": "jeremylarter@jeremylarter.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@cycle/dom": "^12.2.5",
    "@cycle/rxjs-run": "^3.0.3",
    "chai": "^3.5.0",
    "jslint": "^0.10.3",
    "mocha": "^3.1.0",
    "rxjs": "^5.0.0-beta.12",
    "sinon": "^1.17.6",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "xstream": "^6.4.1"
  },
  "scripts": {
    "build": "webpack --debug --devtool source-map --output-pathinfo",
    "build:watch": "webpack --display-modules --progress --debug --devtool source-map --output-pathinfo --watch",
    "build:production": "webpack --config webpack-production.config.js --optimize-minimize --optimize-occurence-order",
    "build:test": "tsc -p tsconfig.test.json",
    "test": "npm run build:test && npm run test:runner",
    "test:runner": "mocha",
    "test:debug": "mocha --debug-brk",
    "test:inspect": "mocha --debug-brk --inspect",
    "tsc": "tsc",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.45",
    "@types/sinon": "^1.16.31",
    "normalize.css": "^4.2.0"
  }
}
